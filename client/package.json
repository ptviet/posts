{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon server/index.ts",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "@types/jest": "^23.3.13",
    "@types/lodash": "^4.14.120",
    "@types/next": "^7.0.6",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/styled-components": "^4.1.6",
    "@types/webpack-env": "^1.13.6",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-import": "^1.11.0",
    "lodash": "^4.17.11",
    "next": "canary",
    "next-images": "^1.0.4",
    "next-routes": "^1.4.2",
    "react": "next",
    "react-dom": "next",
    "react-scripts": "^2.1.3",
    "styled-components": "^4.1.3",
    "ts-log-debug": "^4.0.4"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-react-hooks": "0.0.0",
    "jest": "^23.6.0",
    "jest-transform-graphql": "^2.1.0",
    "next-plugin-transpile-modules": "^1.0.0",
    "nodemon": "^1.18.9",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.4",
    "enzyme-to-json": "^3.3.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.(test|spec).(ts|tsx)"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "tsConfig": "jest.tsconfig.json"
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "jest.setup.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/mocks.ts",
      "\\.(css|less)$": "<rootDir>/__mocks__/mocks.ts"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
