{
  "name": "posts",
  "version": "0.1.0",
  "description": "posts",
  "private": true,
  "scripts": {
    "dev": "nodemon server/server.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node .next/production-server/server.js",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.0",
    "@material-ui/icons": "^3.0.2",
    "@types/classnames": "^2.2.7",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.13",
    "@types/lodash": "^4.14.120",
    "@types/next": "^7.0.6",
    "@types/node": "^10.12.18",
    "@types/nprogress": "0.0.29",
    "@types/prop-types": "^15.5.8",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-jss": "^8.6.0",
    "@types/react-slick": "^0.23.2",
    "@types/styled-jsx": "^2.2.7",
    "@types/webpack-env": "^1.13.6",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "apollo-boost": "^0.1.27",
    "babel-plugin-import": "^1.11.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "downshift": "^3.2.1",
    "express": "^4.16.4",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "next": "canary",
    "next-images": "^1.0.4",
    "next-routes": "^1.4.2",
    "next-with-apollo": "^3.3.2",
    "nodemon": "^1.18.9",
    "notistack": "^0.4.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "react": "next",
    "react-apollo": "^2.4.0",
    "react-dom": "next",
    "react-jss": "^8.6.1",
    "react-responsive-masonry": "^2.0.0",
    "react-scripts": "^2.1.3",
    "react-slick": "^0.23.2",
    "styled-jsx": "^3.2.0",
    "ts-log-debug": "^4.0.4",
    "typescript": "^3.2.4",
    "waait": "^1.0.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-react-hooks": "0.0.0",
    "jest": "^23.6.0",
    "jest-transform-graphql": "^2.1.0",
    "next-plugin-transpile-modules": "^1.0.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "enzyme-to-json": "^3.3.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.(test|spec).(ts|tsx)"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "tsConfig": "jest.tsconfig.json"
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "jest.setup.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/mocks.ts",
      "\\.(css|less)$": "<rootDir>/__mocks__/mocks.ts"
    }
  },
  "engines": {
    "node": "11.6.0",
    "npm": "6.5.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
